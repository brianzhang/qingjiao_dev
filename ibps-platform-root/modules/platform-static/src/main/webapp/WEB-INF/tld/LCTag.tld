	<function>
		<description>获取地理区域对象列表</description>
		<name>getAreaListJson</name>
		<function-class>com.lc.ibps.platform.cat.utils.AreaUtil
		</function-class>
		<function-signature>java.lang.String getAreaListJson()
		</function-signature>
		<example>${f:getAreaListJson()}</example>
	</function>

	<!--样式标签 -->
	<tag>
		<description>样式标签</description>
		<name>link</name>
		<tag-class> com.lc.ibps.web.tag.StyleTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>href</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>isCommon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>权限标签</description>
		<name>a</name>
		<tag-class> com.lc.ibps.web.tag.AnchorTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>alias</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>classes</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>href</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>target</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>action</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>onclick</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>showNoRights</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>
