	<function>
		<description>获取地理区域对象列表</description>
		<name>getAreaList</name>
		<function-class>com.lc.ibps.platform.cat.utils.AreaUtil
		</function-class>
		<function-signature>java.util.List getAreaList()
		</function-signature>
		<example>${f:getAreaList()}</example>
