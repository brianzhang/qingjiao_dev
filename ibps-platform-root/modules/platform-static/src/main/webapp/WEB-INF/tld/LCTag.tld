	<function>
		<description>获取地理区域值</description>
		<name>getAreaValue</name>
		<function-class>com.lc.ibps.platform.cat.utils.AreaUtil
		</function-class>
		<function-signature>java.lang.String getAreaValue(java.lang.String,
			java.lang.String)
		</function-signature>
