<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@include file="/commons/include/html_doctype.html" %>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<%@include file="/commons/page/codegen.jsp" %>
		<script type="text/javascript" src="${ctx}/js/lc/pg/PGData/evaluate.js"></script>
	</head>
	<body>
		<!-- 顶部按钮 -->
		<div class="wrapper wrapper-content  animated fadeInRight col-sm-12">
			<div class="panel-toolbar ">
				<div class="buttons">
					<a href="javascript:void(0);" class="btn btn-primary fa fa-save" ><span>保存</span></a>
					<a href="list.htm" class="btn btn-primary fa fa-back" ><span>返回</span></a>
				</div>
			</div>
			<div class="">
				<form  class="fr-form"  id="evaluateForm" action="save.htm" >
					<input type="hidden" name="m:evaluate:id"  value="${evaluate.id}"/>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">创建时间</label>
				  	<div class="fr-form-block">
				<div class="input-icon" >
					<i class="fa fa-calendar"></i>
					<input type="text" readonly="readonly" class="fr-form-control datepicker" datefmt="yyyy-MM-dd"   name="m:evaluate:createTime"   value="<fmt:formatDate value="${evaluate.createTime}"  pattern="yyyy-MM-dd"/>" validate="{required:false}"/>
				</div>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">课程ID</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:crs_id" value="${evaluate.crs_id}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">教师</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:teacher" value="${evaluate.teacher}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">形式</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:forms" value="${evaluate.forms}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">比例1</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:proportion1" value="${evaluate.proportion1}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">比例2</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:proportion2" value="${evaluate.proportion2}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">比例3</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:proportion3" value="${evaluate.proportion3}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">合理性判定</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:decide" value="${evaluate.decide}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">其他</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:other" value="${evaluate.other}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">判定结果</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:result" value="${evaluate.result}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">弱项</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:ws" value="${evaluate.ws}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
			 	<div class="fr_response_field col-sm-12" >
			 	<div class="fr-form-group"> 
				 	<label class="fr-control-label">负责人</label>
				  	<div class="fr-form-block">
				<input type="text" class="fr-form-control" name="m:evaluate:principle" value="${evaluate.principle}" validate="{required:false}"/>
				 	</div>
			  	</div>
			</div>
</form>

			</div>
		</div>
	</body>
</html>
